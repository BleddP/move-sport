{
  "kind": "collectionType",
  "collectionName": "contact_forms",
  "info": {
    "singularName": "contact-form",
    "pluralName": "contact-forms",
    "displayName": "Contact Form",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "maxLength": 25,
      "minLength": 2
    },
    "email": {
      "type": "email",
      "required": true,
      "maxLength": 55,
      "minLength": 5
    },
    "phone": {
      "type": "string",
      "maxLength": 12,
      "minLength": 6
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "individueel",
        "zakelijk",
        "teams"
      ],
      "default": "individueel",
      "required": true
    },
    "message": {
      "type": "text",
      "maxLength": 500
    },
    "date": {
      "type": "datetime"
    },
    "returned_call": {
      "type": "boolean",
      "default": false
    }
  }
}
